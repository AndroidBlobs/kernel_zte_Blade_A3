	io-channels = <&pmic_adc 7>,<&pmic_adc 10>,
			<&pmic_adc 36>,<&pmic_adc 16>,<&pmic_adc 1>;
	io-channel-names = "adc_temp","adc_vbat",
			"adc_vchg","adc_isense","adc_id";
	sprd,prechg-led-gpios = <&ap_gpio 33 0>;
	battery-adapt-support = <0>;
	battery-adapt-fun = <0>;
	adp-cdp-cur = <1000>;
	adp-dcp-cur = <1000>;
	adp-sdp-cur = <500>;
	adp-unknown-cur = <500>;
	chg-end-vol = <4350>;
	chg-end-vol-check = <4330>;
	chg-bat-safety-vol = <4430>;
	rechg-vol = <4270>;
	chg-end-vol-z = <4100>;
	chg-end-cur-z = <80>;
	rechg-vol-z = <4020>;
	rsense-real = <200>;
	rint = <197>;
	cnom = <2645>;
	chg-polling-time = <10>;
	jeita-temp-tab = <1000 1100 1150 1450 1580>;
	jeita-temp-recovery-tab = <1030 1130 1180 1420 1550>;
	jeita-cur-tab = <0 780 780 1000 1000 0>;
	jeita-cccv-tab = <4350 4350 4350 4350 4100 4100>;
	ocv-tab-vol = <4324 4256 4199 4146 4094 4055 3986
			3960 3922 3874 3839 3817 3800 3786
			3775 3762 3744 3721 3691 3678 3400>;
	ocv-tab-cap = <100  95   90   85   80   75   70
			65   60   55   50   45   40   35
			30   25   20   15   10   5   0>;
};

&pmic_fgu {
	ocv-type = <1>;
};

&sprd_audio_codec_ana {
	status = "okay";
	digital-codec = <&sprd_audio_codec_dig>;
};

&sound_vbc_r1p0v3_sprd_codec {
