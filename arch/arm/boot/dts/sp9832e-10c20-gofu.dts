&sound_sprd_ap_alliis {
	status = "okay";
};

&sprd_headset {
	status = "okay";
};

&i2c2 {
	status = "okay";
	clock-frequency = <400000>;

	acc_gyro_bmi160@69{
		compatible = "bosch-sensortec,bmi160";
		reg = <0x69>;
		gpios = <&ap_gpio 55 0>;
	};
	icm206xx@68{
		compatible = "st,icm206xx";
		reg = <0x68>;
		//st,drdy-int-pin = <1>;
	};
	acc_gyro_lsm6dsl@6B{
		compatible = "st,lsm6dsl";
		reg = <0x6B>;
	};
	ltr_558als@23{
		compatible = "LITEON,ltr_558als";
		reg = <0x23>;
		gpios = <&ap_gpio 52 0>;
		sensitive = <1500 100 60
					1800 1700 48>;
		luxcorrection = <2330>;
	};
};

&i2c3 {
        status = "okay";
        clock-frequency = <400000>;
	adaptive-touchscreen@38 {
		compatible = "adaptive-touchscreen";
		reg = <0x38>;
		status = "disabled";
		gpios = <&ap_gpio 145 0
			&ap_gpio 144 0>;
		controller = "focaltech,FT5436";
		virtualkeys = <580 60 1320 60 45
				172 360 1320 60 45
				158 600 1320 60 45>;
		surface-width = <720>;
		surface-height = <1280>;
		private-data {
		a8 = /bits/ 8 <0x5A>;
		};
	};
	synaptics_dsx@22 {
		compatible = "synaptics,dsx-i2c";
		reg = <0x22>;
		interrupt-parent = <&ap_gpio>;
		interrupts = <144 0x2008>;
		synaptics,irq-gpio = <&ap_gpio 144 0x2008>; /* IRQF_ONESHOT | IRQF_TRIGGER_LOW */
		synaptics,reset-gpio = <&ap_gpio 145 0>;
		synaptics,irq-on-state = <0>;
		synaptics,power-delay-ms = <200>;
		synaptics,reset-delay-ms = <200>;
		synaptics,reset-on-state = <0>;
		synaptics,reset-active-ms = <20>;
	};
};

&i2c4 {
	status = "okay";
	clock-frequency = <400000>;
	flash_ic: flash-ic@63 {
		compatible = "sprd,flash-aw3648";
		reg = <0x63>;
		sprd,flash-ic = <3648>;
		sprd,torch = <1>;
		sprd,preflash = <1>;
		sprd,highlight = <1>;
		sprd,torch-level = <10>;
		sprd,preflash-level = <15>;
		sprd,highlight-level = <15>;
		sprd,lvfm-enable = <1>;
		flash-torch-en-gpios = <&ap_gpio 76 0>;
		flash-chip-en-gpios = <&ap_gpio 89 0>;
		flash-en-gpios = <&ap_gpio 76 0>;
		flash-sync-gpios = <&ap_gpio 76 0>;
	};

	fan54015_chg: charger@6a {
		compatible = "fairchild,fan54015_chg";
		reg = <0x6a>;
		vbus-det-gpios = <&pmic_eic 0 0>;
	};
};

